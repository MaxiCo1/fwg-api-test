{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "OPTIONS", "PUT", "DELETE"]
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/debug-env",
      "dest": "/api/debug-env.js"
    }
  ]
}